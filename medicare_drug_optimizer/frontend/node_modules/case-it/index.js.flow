/* @flow strict */

/**
 * Convert a `string` to camel case.
 */
declare export function camelCaseIt(str: string): string

/**
 * Convert a `string` to constant case.
 */
declare export function constantCaseIt(str: string): string

/**
 * Convert a `string` to dot case.
 */
declare export function dotCaseIt(str: string): string

/**
 * Convert a `string` to kebab case.
 */
declare export function kebabCaseIt(str: string): string

/**
 * Remove any starting case from a `string`, like camel or snake, but keep
 * spaces and punctuation that may be important otherwise.
 */
declare export function noCaseIt(str: string): string

/**
 * Convert a `string` to pascal case.
 */
declare export function pascalCaseIt(str: string): string

/**
 * Convert a `string` to snake case.
 */
declare export function snakeCaseIt(str: string): string

/**
 * Convert a `string` to space case.
 */
declare export function spaceCaseIt(str: string): string